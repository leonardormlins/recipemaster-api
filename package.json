{
  "name": "recipemaster-api",
  "version": "1.0.0",
  "description": "Typescript RecipeMaster API",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:dev": "ts-node-dev 'src/index.ts'",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "test:functional": "jest --projects ./test --runInBand",
    "test:unit": "jest",
    "style:check": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
    "style:fix": "prettier --write 'src/**/*.ts' 'test/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leonardormlins/recipemaster-api.git"
  },
  "keywords": [
    "typescript",
    "api",
    "recipes",
    "node"
  ],
  "author": "github.com/leonardormlins",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/leonardormlins/recipemaster-api/issues"
  },
  "homepage": "https://github.com/leonardormlins/recipemaster-api#readme",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/config": "^0.0.38",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/module-alias": "^2.0.0",
    "@types/mongoose": "^5.10.3",
    "@types/node": "^14.14.22",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.18.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.5",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "config": "^3.3.3",
    "express": "^4.17.1",
    "module-alias": "^2.2.2",
    "mongoose": "^5.11.14"
  }
}
